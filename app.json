{
    "name": "Thani-Oruvan-Mirror-Leech",
    "description": "A Telegram bot for all your mirror needs",
    "logo": "https://telegra.ph/file/019996f816db9ed576cff.jpg",
    "keywords": [
      "bot",
      "telegram",
      "python"
    ],
    "repository": "https://github.com/thanioruvangkls/Thani-Oruvan-Mirror-Leech",
    "website": "https://github.com/thanioruvangkls/Thani-Oruvan-Mirror-Leech",
    "stack": "container",
    "env": {
      "ENV": {
        "description": "Setting this to ANYTHING will enable Webhooks when in env mode",
        "value": "ANYTHING"
      },
      "CONFIG_FILE_URL": {
        "description": "git.gist.com raw link without commit id",
        "required": true
      }
    },
    "addons": [
      {
        "plan": "heroku-postgresql"
      }
    ],
    "formation": {
            "worker": {
                "quantity": 1,
                "size": "free"
            }
      }
}
